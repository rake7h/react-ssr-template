{
  "name": "react-ssr-template",
  "version": "1.0.0",
  "description": "react app with ssr rendring enabled ",
  "main": "index.js",
  "repository": "https://github.com/rake7h/react-ssr-template.git",
  "author": "Rakesh <hi@rake7h.com>",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:serve": "nodemon --watch build --exec \"node build/server/index.js\"",
    "dev:build-client": "webpack --config ./webpack.client.dev.js --hot --watch",
    "dev:build-server": "webpack --config ./webpack.server.js --watch",
    "build": "rm -rf build && npm-run-all --serial prod:build-*",
    "prod:": "node 'build/server/index.js'",
    "prod:build-client": "webpack --config ./webpack.client.prod.js",
    "prod:build-server": "webpack --config ./webpack.server.js"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "babel-loader": "^8.2.3",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.27.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^4.6.0"
  }
}
